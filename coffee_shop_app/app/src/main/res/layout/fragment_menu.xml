<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <data>
        <import type="android.view.View" />
        <variable
            name="menuViewModel"
            type="com.example.coffee_shop_app.viewmodels.MenuViewModel"/>
        <variable
            name="cartButtonViewModel"
            type="com.example.coffee_shop_app.viewmodels.CartButtonViewModel" />
    </data>
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".fragments.MenuFragment">
        <!-- This component for getting focusable -->
        <Button
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:focusable="true"
            android:focusableInTouchMode="true"/>
        <!-- This component for getting focusable-->
        <androidx.swiperefreshlayout.widget.SwipeRefreshLayout
            android:id="@+id/refreshLayout"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:scrollbars="none">
            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <com.facebook.shimmer.ShimmerFrameLayout
                    android:id="@+id/shimmer_layout"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:orientation="vertical"
                    android:visibility="@{menuViewModel.loading?View.VISIBLE:View.GONE}"
                    android:background="@color/background">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:paddingHorizontal="@dimen/_16sdp"
                        android:paddingVertical="@dimen/_6sdp">

                        <include layout="@layout/product_item_skeleton" />

                        <include layout="@layout/product_item_skeleton" />

                        <include layout="@layout/product_item_skeleton" />

                        <include layout="@layout/product_item_skeleton" />

                        <include layout="@layout/product_item_skeleton" />

                        <include layout="@layout/product_item_skeleton" />

                        <include layout="@layout/product_item_skeleton" />

                        <include layout="@layout/product_item_skeleton" />

                        <include layout="@layout/product_item_skeleton" />
                    </LinearLayout>
                </com.facebook.shimmer.ShimmerFrameLayout>
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    android:visibility="@{(cartButtonViewModel.delivering&amp;&amp;menuViewModel.loading == false)?View.VISIBLE:View.GONE}">
                    <androidx.core.widget.NestedScrollView
                        android:id="@+id/delivery_nested_scroll_view"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintRight_toRightOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"
                        android:background="@color/white">
                        <androidx.constraintlayout.widget.ConstraintLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">
                            <androidx.constraintlayout.widget.ConstraintLayout
                                android:id="@+id/delivery_address_picker_frame"
                                app:layout_constraintTop_toTopOf="parent"
                                app:layout_constraintLeft_toLeftOf="parent"
                                app:layout_constraintRight_toRightOf="parent"
                                app:layout_constraintBottom_toTopOf="@id/delivery_store_picker_frame"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                android:paddingHorizontal="@dimen/_16sdp"
                                android:paddingVertical="@dimen/_8sdp">
                                <ImageView
                                    android:id="@+id/delivery_address_picker_imageview"
                                    app:layout_constraintTop_toTopOf="parent"
                                    app:layout_constraintLeft_toLeftOf="parent"
                                    app:layout_constraintRight_toLeftOf="@id/delivery_address_picker_info"
                                    android:layout_width="@dimen/_40sdp"
                                    android:layout_height="@dimen/_40sdp"
                                    android:src="@drawable/img_delivery_small_icon"
                                    android:layout_marginEnd="@dimen/_8sdp"
                                    android:contentDescription="@null"/>
                                <LinearLayout
                                    android:id="@+id/delivery_address_picker_info"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:orientation="vertical"
                                    app:layout_constraintBottom_toBottomOf="parent"
                                    app:layout_constraintRight_toLeftOf="@id/delivery_address_picker_right_arrow"
                                    app:layout_constraintLeft_toRightOf="@id/delivery_address_picker_imageview"
                                    app:layout_constraintTop_toTopOf="parent">

                                    <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:text="Giao hàng đến"
                                        style="@style/regularBlack14"/>
                                    <View
                                        android:layout_width="wrap_content"
                                        android:layout_height="@dimen/_4sdp"/>
                                    <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:text="@{cartButtonViewModel.userAddress}"
                                        android:ellipsize="end"
                                        android:maxLines="1"
                                        style="@style/boldBlack14"/>
                                    <View
                                        android:layout_width="wrap_content"
                                        android:layout_height="@dimen/_4sdp"/>
                                    <TableLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:shrinkColumns="0">
                                        <TableRow
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:gravity="center_vertical">
                                            <TextView
                                                android:id="@+id/name_text_view"
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:maxLines="1"
                                                android:ellipsize="end"
                                                android:text="@{cartButtonViewModel.nameReceiver}"
                                                style="@style/regularGrey14"/>
                                            <TextView
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:maxLines="1"
                                                android:ellipsize="none"
                                                android:text=" • "
                                                style="@style/regularGrey14">
                                            </TextView>
                                            <TextView
                                                android:id="@+id/phone_text_view"
                                                android:layout_width="wrap_content"
                                                android:layout_height="wrap_content"
                                                android:maxLines="1"
                                                android:ellipsize="none"
                                                android:text="@{cartButtonViewModel.phone}"
                                                style="@style/regularGrey14"/>
                                        </TableRow>
                                    </TableLayout>
                                </LinearLayout>

                                <TextView
                                    android:id="@+id/delivery_address_picker_right_arrow"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="@dimen/_8sdp"
                                    android:fontFamily="@font/font_awesome_free_solid_900"
                                    android:text="chevron-right"
                                    android:textSize="@dimen/_16sdp"
                                    app:layout_constraintBottom_toBottomOf="parent"
                                    app:layout_constraintLeft_toRightOf="@id/delivery_address_picker_info"
                                    app:layout_constraintRight_toRightOf="parent"
                                    app:layout_constraintTop_toTopOf="parent" />
                            </androidx.constraintlayout.widget.ConstraintLayout>
                            <androidx.constraintlayout.widget.ConstraintLayout
                                android:id="@+id/delivery_store_picker_frame"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                app:layout_constraintTop_toBottomOf="@id/delivery_address_picker_frame"
                                app:layout_constraintBottom_toTopOf="@id/delivery_products_linear_layout"
                                app:layout_constraintLeft_toLeftOf="parent"
                                app:layout_constraintRight_toRightOf="parent"
                                android:paddingHorizontal="@dimen/_16sdp"
                                android:paddingVertical="@dimen/_8sdp"
                                android:background="@color/white">
                                <androidx.constraintlayout.widget.ConstraintLayout
                                    android:id="@+id/delivery_store_picker"
                                    app:layout_constraintTop_toTopOf="parent"
                                    app:layout_constraintLeft_toLeftOf="parent"
                                    app:layout_constraintRight_toRightOf="parent"
                                    app:layout_constraintBottom_toBottomOf="parent"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="horizontal"
                                    android:paddingVertical="@dimen/_8sdp"
                                    android:background="@drawable/border_dddde3_4_ffffff"
                                    android:paddingStart="@dimen/_4sdp"
                                    android:paddingEnd="@dimen/_8sdp">
                                    <TextView
                                        android:id="@+id/delivery_store_picker_text_store"
                                        app:layout_constraintLeft_toLeftOf="parent"
                                        app:layout_constraintRight_toLeftOf="@id/delivery_separater"
                                        app:layout_constraintTop_toTopOf="parent"
                                        app:layout_constraintBottom_toBottomOf="parent"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:paddingHorizontal="@dimen/_8sdp"
                                        android:paddingVertical="@dimen/_1sdp"
                                        android:text="Cửa hàng"
                                        style="@style/regularBlack14"/>
                                    <View
                                        android:id="@+id/delivery_separater"
                                        app:layout_constraintLeft_toRightOf="@id/delivery_store_picker_text_store"
                                        app:layout_constraintRight_toLeftOf="@id/delivery_store_picker_text_address"
                                        app:layout_constraintTop_toTopOf="parent"
                                        app:layout_constraintBottom_toBottomOf="parent"
                                        android:layout_width="@dimen/_1sdp"
                                        android:layout_height="@dimen/_24sdp"
                                        android:background="@color/grey_border"/>
                                    <TextView
                                        android:id="@+id/delivery_store_picker_text_address"
                                        android:layout_width="0dp"
                                        android:layout_height="match_parent"
                                        app:layout_constraintLeft_toRightOf="@id/delivery_separater"
                                        app:layout_constraintRight_toLeftOf="@id/delivery_store_picker_icon"
                                        app:layout_constraintTop_toTopOf="parent"
                                        app:layout_constraintBottom_toBottomOf="parent"
                                        android:layout_marginHorizontal="@dimen/_8sdp"
                                        android:ellipsize="end"
                                        android:maxLines="1"
                                        android:text="@{cartButtonViewModel.storeAddress}"
                                        style="@style/regularBlack14"/>
                                    <TextView
                                        android:id="@+id/delivery_store_picker_icon"
                                        app:layout_constraintBottom_toBottomOf="parent"
                                        app:layout_constraintLeft_toRightOf="@id/delivery_store_picker_text_address"
                                        app:layout_constraintRight_toRightOf="parent"
                                        app:layout_constraintTop_toTopOf="parent"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginEnd="@dimen/_8sdp"
                                        android:fontFamily="@font/font_awesome_free_solid_900"
                                        android:text="chevron-down"
                                        android:textSize="16dp"/>
                                </androidx.constraintlayout.widget.ConstraintLayout>
                            </androidx.constraintlayout.widget.ConstraintLayout>
                            <LinearLayout
                                android:id="@+id/delivery_products_linear_layout"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:nestedScrollingEnabled="false"
                                app:layout_constraintTop_toBottomOf="@id/delivery_store_picker_frame"
                                app:layout_constraintLeft_toLeftOf="parent"
                                app:layout_constraintRight_toRightOf="parent"
                                app:layout_constraintBottom_toBottomOf="parent"
                                android:orientation="vertical"
                                android:layout_marginHorizontal="@dimen/_16sdp">
                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:paddingVertical="@dimen/_12sdp"
                                    android:text="Yêu thích"
                                    style="@style/mediumBlack14"
                                    android:visibility="@{menuViewModel.hasFavoriteFood?View.VISIBLE:View.GONE}"/>
                                <androidx.recyclerview.widget.RecyclerView
                                    android:id="@+id/delivery_product_item_favorites_recycler_view"
                                    android:nestedScrollingEnabled="false"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"/>
                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:paddingVertical="@dimen/_12sdp"
                                    android:text="Khác"
                                    style="@style/mediumBlack14"
                                    android:visibility="@{menuViewModel.hasOtherFood?View.VISIBLE:View.GONE}"/>
                                <androidx.recyclerview.widget.RecyclerView
                                    android:id="@+id/delivery_product_item_recycler_view"
                                    android:nestedScrollingEnabled="false"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"/>
                                <View
                                    android:layout_width="match_parent"
                                    android:layout_height="@dimen/_68sdp"/>
                            </LinearLayout>
                        </androidx.constraintlayout.widget.ConstraintLayout>
                    </androidx.core.widget.NestedScrollView>
                </androidx.constraintlayout.widget.ConstraintLayout>
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    android:visibility="@{(cartButtonViewModel.delivering==false&amp;&amp;menuViewModel.loading == false)?View.VISIBLE:View.GONE}">
                    <androidx.core.widget.NestedScrollView
                        android:id="@+id/pick_up_nested_scroll_view"
                        android:layout_width="match_parent"
                        android:layout_height="0dp"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintRight_toRightOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"
                        android:background="@color/white">
                        <androidx.constraintlayout.widget.ConstraintLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">
                            <androidx.constraintlayout.widget.ConstraintLayout
                                android:id="@+id/pick_up_store_pickup_picker"
                                app:layout_constraintTop_toTopOf="parent"
                                app:layout_constraintLeft_toLeftOf="parent"
                                app:layout_constraintRight_toRightOf="parent"
                                app:layout_constraintBottom_toTopOf="@id/pick_up_products_linear_layout"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                android:paddingHorizontal="@dimen/_16sdp"
                                android:paddingVertical="@dimen/_8sdp">
                                <ImageView
                                    android:id="@+id/pick_up_store_pickup_picker_imageview"
                                    app:layout_constraintTop_toTopOf="parent"
                                    app:layout_constraintLeft_toLeftOf="parent"
                                    app:layout_constraintRight_toLeftOf="@id/pick_up_store_pickup_picker_info"
                                    android:layout_width="@dimen/_40sdp"
                                    android:layout_height="@dimen/_40sdp"
                                    android:src="@drawable/img_pickup_small_icon"
                                    android:layout_marginEnd="8dp"/>
                                <LinearLayout
                                    android:id="@+id/pick_up_store_pickup_picker_info"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:orientation="vertical"
                                    app:layout_constraintBottom_toBottomOf="parent"
                                    app:layout_constraintRight_toLeftOf="@id/pick_up_address_picker_right_arrow"
                                    app:layout_constraintLeft_toRightOf="@+id/pick_up_store_pickup_picker_imageview"
                                    app:layout_constraintTop_toTopOf="parent">

                                    <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:text="Đến lấy tại"
                                        style="@style/regularBlack14"/>
                                    <View
                                        android:layout_width="wrap_content"
                                        android:layout_height="@dimen/_4sdp"/>
                                    <TextView
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:text="@{cartButtonViewModel.storeAddress}"
                                        android:ellipsize="end"
                                        android:maxLines="1"
                                        style="@style/boldBlack14"/>
                                </LinearLayout>

                                <TextView
                                    android:id="@+id/pick_up_address_picker_right_arrow"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="@dimen/_8sdp"
                                    android:fontFamily="@font/font_awesome_free_solid_900"
                                    android:text="chevron-right"
                                    android:textSize="@dimen/font16"
                                    app:layout_constraintBottom_toBottomOf="parent"
                                    app:layout_constraintLeft_toRightOf="@id/pick_up_store_pickup_picker_info"
                                    app:layout_constraintRight_toRightOf="parent"
                                    app:layout_constraintTop_toTopOf="parent" />
                            </androidx.constraintlayout.widget.ConstraintLayout>
                            <LinearLayout
                                android:id="@+id/pick_up_products_linear_layout"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:nestedScrollingEnabled="false"
                                app:layout_constraintTop_toBottomOf="@id/pick_up_store_pickup_picker"
                                app:layout_constraintLeft_toLeftOf="parent"
                                app:layout_constraintRight_toRightOf="parent"
                                app:layout_constraintBottom_toBottomOf="parent"
                                android:orientation="vertical"
                                android:layout_marginHorizontal="@dimen/_16sdp">
                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:paddingVertical="@dimen/_12sdp"
                                    android:text="Yêu thích"
                                    style="@style/mediumBlack14"
                                    android:visibility="@{menuViewModel.hasFavoriteFood?View.VISIBLE:View.GONE}"/>
                                <androidx.recyclerview.widget.RecyclerView
                                    android:id="@+id/pick_up_product_item_favorites_recycler_view"
                                    android:nestedScrollingEnabled="false"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"/>
                                <TextView
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:paddingVertical="@dimen/_12sdp"
                                    android:text="Khác"
                                    style="@style/mediumBlack14"
                                    android:visibility="@{menuViewModel.hasOtherFood?View.VISIBLE:View.GONE}"/>
                                <androidx.recyclerview.widget.RecyclerView
                                    android:id="@+id/pick_up_product_item_recycler_view"
                                    android:nestedScrollingEnabled="false"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"/>
                                <View
                                    android:layout_width="match_parent"
                                    android:layout_height="@dimen/_68sdp"/>
                            </LinearLayout>
                        </androidx.constraintlayout.widget.ConstraintLayout>
                    </androidx.core.widget.NestedScrollView>
                </androidx.constraintlayout.widget.ConstraintLayout>
                <com.google.android.material.card.MaterialCardView
                    android:id="@+id/cart_button"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_8sdp"
                    android:layout_marginHorizontal="@dimen/_16sdp"
                    android:orientation="horizontal"
                    app:cardElevation="@dimen/_2sdp"
                    app:cardCornerRadius="@dimen/_4sdp">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_margin="@dimen/_8sdp">
                        <androidx.constraintlayout.motion.widget.MotionLayout
                            android:layout_weight="1"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            app:layoutDescription="@xml/cart_button_address_info_motion_scene"
                            android:id="@+id/address_info_motion_layout">
                            <ImageView
                                android:id="@+id/image_order_type"
                                app:layout_constraintTop_toTopOf="parent"
                                app:layout_constraintBottom_toBottomOf="parent"
                                app:layout_constraintLeft_toLeftOf="parent"
                                android:layout_marginBottom="@dimen/_20sdp"
                                android:layout_width="@dimen/_20sdp"
                                android:layout_height="@dimen/_20sdp"
                                android:contentDescription="@null"
                                android:src="@{cartButtonViewModel.delivering?@drawable/img_delivery_small_icon:@drawable/img_pickup_small_icon}"/>
                            <TextView
                                android:id="@+id/text_order_type"
                                app:layout_constraintTop_toTopOf="parent"
                                app:layout_constraintLeft_toRightOf="@id/image_order_type"
                                app:layout_constraintRight_toRightOf="parent"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/_4sdp"
                                android:text="@{cartButtonViewModel.delivering ? @string/delivery_to : @string/pick_up_from}"
                                style="@style/mediumGrey12"/>
                            <TextView
                                android:id="@+id/address_text_view"
                                app:layout_constraintBottom_toBottomOf="parent"
                                app:layout_constraintLeft_toLeftOf="parent"
                                app:layout_constraintRight_toRightOf="parent"
                                android:layout_marginTop="@dimen/_20sdp"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="@{cartButtonViewModel.delivering ? cartButtonViewModel.userAddress : cartButtonViewModel.storeAddress}"
                                style="@style/mediumBlack12"
                                android:maxLines="1"
                                android:ellipsize="end"/>
                        </androidx.constraintlayout.motion.widget.MotionLayout>
                        <Space
                            android:layout_width="@dimen/_8sdp"
                            android:layout_height="match_parent"/>
                        <androidx.constraintlayout.motion.widget.MotionLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:background="@drawable/button_border_none_1a94ff"
                            app:layoutDescription="@xml/cart_button_price_and_amount_motion_scene"
                            android:id="@+id/cart_button_price_and_amount_motion_layout"
                            android:visibility="@{cartButtonViewModel.hasFoodInCart?View.VISIBLE:View.GONE}">
                            <LinearLayout
                                android:id="@+id/cart_button_price_and_amount_linear"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                app:layout_constraintTop_toTopOf="parent"
                                app:layout_constraintRight_toRightOf="parent"
                                app:layout_constraintLeft_toLeftOf="parent"
                                app:layout_constraintBottom_toBottomOf="parent"
                                android:layout_marginHorizontal="@dimen/_8sdp"
                                android:layout_marginBottom="@dimen/_6sdp"
                                android:layout_marginTop="@dimen/_6sdp">
                                <TextView
                                    android:layout_height="@dimen/_16sdp"
                                    android:layout_width="@dimen/_16sdp"
                                    android:layout_gravity="center_vertical"
                                    android:gravity="center"
                                    android:padding="@dimen/_1sdp"
                                    android:textColor="@color/blue"
                                    android:fontFamily="@font/inter_medium"
                                    android:autoSizeTextType="uniform"
                                    android:autoSizeMaxTextSize="@dimen/_12sdp"
                                    android:autoSizeMinTextSize="@dimen/_4sdp"
                                    android:autoSizeStepGranularity="@dimen/_2sdp"
                                    android:text="@{cartButtonViewModel.numberFoodString}"
                                    android:background="@drawable/button_border_1a94ff_ffffff"/>
                                <Space
                                    android:layout_width="@dimen/_4sdp"
                                    android:layout_height="match_parent"/>
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="match_parent"
                                    android:text="@{cartButtonViewModel.totalFoodString}"
                                    style="@style/mediumWhite12"/>
                                <Space
                                    android:layout_width="@dimen/_4sdp"
                                    android:layout_height="match_parent"/>
                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="match_parent"
                                    android:text="chevron-right"
                                    android:gravity="center_vertical"
                                    android:textColor="@color/white"
                                    android:fontFamily="@font/font_awesome_free_solid_900"
                                    android:textSize="@dimen/font10"/>
                                <Space
                                    android:layout_width="@dimen/_2sdp"
                                    android:layout_height="match_parent"/>
                            </LinearLayout>
                        </androidx.constraintlayout.motion.widget.MotionLayout>
                    </LinearLayout>
                </com.google.android.material.card.MaterialCardView>
            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.swiperefreshlayout.widget.SwipeRefreshLayout>
    </FrameLayout>
</layout>
